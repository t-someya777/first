  "bugs": {
    "url": "https://github.com/t-someya777/first/issues"
  },
  "homepage": "https://github.com/t-someya777/first#readme",
  "devDependencies": {
    "ansi-colors": "^4.1.3",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "fancy-log": "^2.0.0",
    "globule": "^1.3.4",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.69.5",
    "sharp": "^0.33.2"
  }
}
